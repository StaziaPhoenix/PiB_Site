	</div>
</div>

<div class="container">

	<div class="row">

		<div class="col-lg-9 col-md-9 col-sm-12 col-xs-12">
			<h4>Description</h4>
			<p><%= project.description %></p>
		</div>

		<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
			<img src="<%= project.mainImage %>" alt="project image" id="mainImage">
<!-- 				<% project.images.forEach(function(imgSrc) { %>
					<img src="<%= imgSrc %>">
				<% }); %> -->
		</div>
	</div> <!-- END ROW -->

	<div class="row">
		<div class="col-6">
			<h6>Skills Learned</h6>
			<ul>
				<% project.skillsLearned.forEach(function(skill) { %>
					<li><%= skill %></li>
				<% }); %>
			</ul>
		</div>
		<div class="col-6">
			<h6>Sequence</h6>
			<ul>
				<% project.sequence.forEach(function(seq) { %>
					<li><a href="<"><%=  %></a></li>
				<% }); %>
			</ul>
		</div>
	</div>

	<a class="btn btn-danger btn-md">Build It</a>

</div>

<% include partials/footer %>